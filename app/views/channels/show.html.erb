<div class="card">
  <div class="card-header">
    <h4><%= t('activerecord.models.channel') %></h4>
  </div>
  <div class="card-body">
    <table class="table table-bordered table-hover autowidth">
    
      <tr>
        <th><%= t('attributes.server')%>:</th>
        <td><%= @channel.server %></td>
      </tr>
    
      <tr>
        <th><%= t('attributes.uid')%>:</th>
        <td><%= @channel.uid %></td>
      </tr>
    
      <tr>
        <th><%= t('attributes.properties')%>:</th>
        <td><%= @channel.properties %></td>
      </tr>
        </table>

    <div role="toolbar">
      <%= back_link %>
      <%= edit_link(@channel) %>
      <%= delete_link(@channel) %>
      <%= new_link Channel %>
    </div>
  </div>
</div>
