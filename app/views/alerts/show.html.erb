<div class="card">
  <div class="card-header">
    <h4><%= t('activerecord.models.alert') %></h4>
  </div>
  <div class="card-body">
    <table class="table table-bordered table-hover autowidth">
    
      <tr>
        <th><%= t('attributes.server')%>:</th>
        <td><%= @alert.server %></td>
      </tr>
    
      <tr>
        <th><%= t('attributes.channel')%>:</th>
        <td><%= @alert.channel %></td>
      </tr>
    
      <tr>
        <th><%= t('attributes.type')%>:</th>
        <td><%= @alert.type %></td>
      </tr>
    
      <tr>
        <th><%= t('attributes.message')%>:</th>
        <td><%= @alert.message %></td>
      </tr>
        </table>

    <div role="toolbar">
      <%= back_link %>
      <%= edit_link(@alert) %>
      <%= delete_link(@alert) %>
      <%= new_link Alert %>
    </div>
  </div>
</div>
