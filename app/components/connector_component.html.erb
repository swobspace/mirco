<div class="card mb-1">
  <div class="card-header">
    <div class="d-flex justify-content-between">
      <span class="<%= enabled %>"><%= mc.name %> (<%= t(mc.enabled, scope: 'enabled') %>)</span>
      <span class="text-muted"><%= mc.transportName %></span>
    </div>
  </div>
  <div class="card-body">
    <table class="connector table table-sm table-borderless">
      <tbody>
        <% mc.descriptor.each do |k,v| %>
          <tr>
            <td class="w-25"><%= k %>: </td>
            <td> <%= v %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>


